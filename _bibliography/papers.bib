@article{aikins2024,
  title        = {LEVIOSA: Natural Language-Based Uncrewed Aerial Vehicle Trajectory Generation},
  author       = {Aikins, Godwyll and Dao, Mawaba Pascal and Moukpe, Koboyo Josias and Eskridge, Thomas C. and Nguyen, Kim-Doang},
  year         = 2024,
  journal      = {Electronics},
  volume       = 13,
  number       = 22,
  doi          = {10.3390/electronics13224508},
  issn         = {2079-9292},
  url          = {https://www.mdpi.com/2079-9292/13/22/4508},
  article-number = 4508,
  abstract     = {This paper presents LEVIOSA, a novel framework for text- and speech-based uncrewed aerial vehicle (UAV) trajectory generation. By leveraging multimodal large language models (LLMs) to interpret natural language commands, the system converts text and audio inputs into executable flight paths for UAV swarms. The approach aims to simplify the complex task of multi-UAV trajectory generation, which has significant applications in fields such as search and rescue, agriculture, infrastructure inspection, and entertainment. The framework involves two key innovations: a multi-critic consensus mechanism to evaluate trajectory quality and a hierarchical prompt structuring for improved task execution. The innovations ensure fidelity to user goals. The framework integrates several multimodal LLMs for high-level planning, converting natural language inputs into 3D waypoints that guide UAV movements and per-UAV low-level controllers to control each UAV in executing its assigned 3D waypoint path based on the high-level plan. The methodology was tested on various trajectory types with promising accuracy, synchronization, and collision avoidance results. The findings pave the way for more intuitive humanâ€“robot interactions and advanced multi-UAV coordination.}
}
